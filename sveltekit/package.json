		"dotenv": "^16.0.3",
		"graphql": "15.8.0",
		"highlight.js": "^11.7.0",
		"highlightjs-svelte": "^1.0.6",
		"marked": "^4.3.0",
		"svelte-markdown": "^0.2.3"
